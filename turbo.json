{
    "$schema": "https://turborepo.com/schema.json",
    "ui": "tui",
    "tasks": {
      "build": {
        "dependsOn": ["^build"],
        "outputs": ["dist/**"]
      },
      "check-types": {
        "dependsOn": ["^check-types"]
      },
      "dev": {
        "persistent": true,
        "cache": false
      },
      "tc": {
        "dependsOn": ["^tc"],
        "cache": false
      }
    }
  }